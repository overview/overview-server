{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "OverviewProductionReadWriteFiles",
            "Effect": "Allow",
            "Action": [
                "s3:DeleteObject",
                "s3:GetObject",
                "s3:PutObject",
                "s3:GetBucketLocation",
                "s3:ListBucketMultipartUploads"
            ],
            "Resource": [
                "arn:aws:s3:::overview-production-file-contents/*",
                "arn:aws:s3:::overview-production-page-data/*",
                "arn:aws:s3:::overview-production-file-view/*"
            ]
        }
    ]
}
